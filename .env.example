# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Session Configuration
SESSION_SECRET=your_random_session_secret_here

# Server Configuration
PORT=3000
BASE_URL=http://localhost:3000

# AI Provider Configuration
# Set to 'lmstudio' for local LM Studio, 'openrouter' for OpenRouter API
# If not set, auto-detects based on which config is available (prefers LM Studio)
AI_PROVIDER=lmstudio

# LM Studio Configuration (local AI - no API key needed)
LMSTUDIO_URL=http://10.5.0.2:1234
LMSTUDIO_MODEL=llama-3.1-deephermes-r1-reasoning-8b-darkidol-instruct-1.2-uncensored

# OpenRouter API Configuration (cloud AI - requires API key)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_DEFAULT_MODEL=google/gemini-2.5-flash-lite-preview-06-17